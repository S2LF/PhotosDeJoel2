{% extends 'base.html.twig' %}

{% block title %}Les Photos de Joël | Admin - Crée lien{% endblock %}

{% block body %}

{% include "admin/adminNav.html.twig" %}

<div class="container">
  <div class="card bg-dark">
    <div class="card-header">
      <h2>Créer/Modifier un lien</h2>
    </div>
    <div class="card-body">
      {{ form_start(form)}}

      {{ form_row(form.title)}}
      {{ form_row(form.link)}}
      <input type="submit" value="Valider">

      {{ form_end(form)}}
    </div>
  </div>
</div>

{% endblock %}